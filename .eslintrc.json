{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "legacyDecorators": true
        }
    },
    "rules": {
        "no-unused-vars": "warn"
    },
    "ignorePatterns": [
        "src/webui/build/**",
        "src/webui/thirdparty",
        "wdio.conf.js",
        "remote-profile/**"
    ]
}
